<cell-diagram xmlns="http://www.cellml.org/celldl/1.0#">

    <!-- Partial representation of diagram at http://sites.bioeng.auckland.ac.nz/mwu035/mpb/  -->

    <diagram>
        <compartment id="cell" class="cell">
            <transporters>
                <transporter id="LFT"/>
                <transporter id="TOP"/>
                <transporter id="RGT"/>
                <transporter id="BOT"/>
            </transporters>

            <compartment id="inner" class="cell">
                <transporters>
                    <transporter id="IL"/>
                    <transporter id="IT"/>
                    <transporter id="IR"/>
                    <transporter id="IB"/>
                </transporters>
            </compartment>
        </compartment>
    </diagram>
    <style>
        diagram {
            width: 1600;
            height: 900;
        }
        #cell, #inner {
            position: (250, 250);
            size: (500, 500);
        }
        #LFT, #IL {
            position: 500 left;
        }
        #TOP, #IT {
            position: 500 top;
        }
        #RGT, #IR {
            position: 500 right;
        }
        #BOT, #IB {
            position: 500 bottom;
        }
    </style>
</cell-diagram>
