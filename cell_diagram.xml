            <flux from="u1" to="u2"/>
        </flow>
        <flow id="v3" transporter="pNa">
            <flux from="u1" to="u2"/>
        </flow>
        <flow id="v4" transporter="NKE">
            <flux from="u2" to="u1" count="3"/>
            <flux from="u4" to="u5" count="2"/>
        </flow>

        <flow id="v6" transporter="NBC">
            <flux from="u7" to="u2"/>
            <flux from="u9" to="u10"/>
        </flow>
        <flow id="v7" transporter="NHE">
            <flux from="u14" to="u13"/>
            <flux from="u1" to="u2"/>
        </flow>
